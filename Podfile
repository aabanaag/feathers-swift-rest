# Uncomment the next line to define a global platform for your project


def framework_pods
  pod 'Feathers'
  pod 'Alamofire'
  pod 'Result'
end

target 'FeathersSwiftRest-iOS' do
  use_frameworks!
  platform :ios, '8.0'
  framework_pods
end

target 'FeathersSwiftRest-macOS' do
  use_frameworks!
  platform :osx, '10.10'
  framework_pods
end

target 'FeathersSwiftRest-tvOS' do
  use_frameworks!
  platform :tvos, '9.0'
  framework_pods
end

target 'FeathersSwiftRest-watchOS' do
  use_frameworks!
  platform :watchos, '2.0'
  framework_pods
end
